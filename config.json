{
    "imports": {
        "data_handler": {
            "name": "filesys",
            "path": "./DataHandler/Filesys/FilesysData.py",
            "module_name": "DataHandler.Filesys"
        }
    },
    "data_root": "./data",
    "models": {
        "post": {
            "route": "posts",
            "data_handler": "filesys",
            "auth_behaviors":
            [
                "Postable",
                "Deleteable",
                "Updateable"
            ],
            "behaviors": [
                "Pageable",
                "Login"
            ],
            "init_params": {
                "header_file": "header",
                "fields": [
                    "header",
                    "content",
                    "md"
                ],
                "non_auth":{
                    "page_size": 5
                }
            }
        }
    }
}